  "scripts": {
    "start": "react-scripts start",
    "git-info": "echo export const gitinfo = \"{\\\"logMessage\\\": \\\"$(git log -1 --oneline)\\\"}\"  > src/_git_commit.js",
    "build": "npm run git-info && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n": "i18next 'src/**/*.{ts,tsx}' -o 'src/locales/$LOCALE/$NAMESPACE.json'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-i18next-extract": "0.8.3"
  }
}
